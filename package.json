    "express": "~4.15.5",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "jquery-validation": "^1.17.0",
    "less-middleware": "~2.2.1",
    "log4js": "^2.3.12",
    "morgan": "~1.9.0",
